<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="simpleTest"
   name="simpleTest Plug-in"
   version="1.0.0"
   provider-name=""
   class="simpleTest.SimpleTestPlugin">

   <runtime>
      <library name="simpleTest.jar">
         <export name="*"/>
      </library>
   </runtime>

   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.core.runtime"/>
   </requires>
   <extension
         id="SimpleTestApplication"
         point="org.eclipse.core.runtime.applications">
   </extension>

</plugin>
